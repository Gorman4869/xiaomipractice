#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../_which@1.3.1@which/bin/which" "$@"
else 
  exec node  "$basedir/../_which@1.3.1@which/bin/which" "$@"
fi
